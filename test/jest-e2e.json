{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/api(.*)$": "<rootDir>/../src/modules/api$1",
    "^@/database(.*)$": "<rootDir>/../src/modules/database$1",
    "^@/shared(.*)$": "<rootDir>/../src/shared$1",
    "^@/worker(.*)$": "<rootDir>/../src/modules/worker$1",
    "^@/queue(.*)$": "<rootDir>/../src/modules/queue$1",
    "^@/sentry(.*)$": "<rootDir>/../src/modules/sentry$1",
    "^@/socket-gateway(.*)$": "<rootDir>/../src/modules/socket-gateway$1",
    "^@test(.*)$": "<rootDir>$1"
  }
}
